{
  "expo": {
    "name": "Reminders",
    "slug": "todo-list-react-native",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "android": {
      "package": "com.ilyomix.reminders"
    },
    "androidStatusBar": {
      "backgroundColor": "#000000"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
